CC=gcc

SRCROOT=../../..
INCLUDE=$(SRCROOT)/include
CCFLAGS=-O3 -Wall -std=gnu99 -I$(INCLUDE) -I.. -DSTDC_HEADERS=1 -DHAVE_UNISTD_H=1 @CFLAGS@

OBJS = gzip.o unzip.o inflate.o util.o unlzw.o unpack.o unlzh.o

all: $(OBJS)

%.o: %.c
	$(CC) $(CCFLAGS) -c $<

clean:
	rm -f *.o *~

distclean:      clean
	rm -f Makefile

depend:
	makedepend -- $(CCFLAGS) -- *.[ch] $(INCLUDE)/*.h ../*.h
