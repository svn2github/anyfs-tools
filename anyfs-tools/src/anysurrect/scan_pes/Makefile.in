CC=gcc

SRCROOT=../../..
INCLUDE=$(SRCROOT)/include
CCFLAGS=-O3 -Wall -std=gnu99 -I.. -I$(INCLUDE) @CFLAGS@

OBJS = aux_pes.o scan_pes.o

all: $(OBJS)

%.o: %.c
	$(CC) $(CCFLAGS) -c $<

clean:
	rm -f *.o *~

distclean:      clean
	rm -f Makefile

depend:
	makedepend -- $(CCFLAGS) -- *.[ch] $(INCLUDE)/*.h ../*.h
